{
  "name": "@amigo/amigo-typescript-sdk",
  "version": "1.0.0",
  "description": "Amigo TypeScript SDK",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "gen-types": "node scripts/gen-types.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "node scripts/gen.mjs && node scripts/build.mjs && tsc",
    "dev": "node scripts/gen.mjs && node scripts/build.mjs --watch",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings 0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amigo-ai/amigo-typescript-sdk.git"
  },
  "author": "Amigo AI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amigo-ai/amigo-typescript-sdk/issues"
  },
  "homepage": "https://github.com/amigo-ai/amigo-typescript-sdk#readme",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "msw": "^2.10.4",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  }
}
